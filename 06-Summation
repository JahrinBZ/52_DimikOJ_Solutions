#include <stdio.h>

int main() {
    int T;
    scanf("%d",&T);
    int a[T];
    int sum = 0;
    for(int i = 0; i< T; i++)
    {
        scanf("%d",&a[i]);
    }
    for (int i = 0 ; i < T; i++)
    {
        printf("Sum = ");
        for (int j = 0; j<T ; j++)
        {
            int num1 = a[i] % 10;
            int num2 = a[i] / 10000;
            sum = num1 + num2;
            
        }
        printf("%d\n",sum);
    }
    return 0;
}
