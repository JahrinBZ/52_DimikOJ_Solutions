#include <stdio.h>

int main() {
    int T;
    scanf("%d",&T);
    int a[T];
    
    for(int i = 0; i< T; i++)
    {
        scanf("%d",&a[i]);
    }
    for (int i = 0 ; i < T; i++)
    {
        for (int row = 0 ; row <a[i] ; row++)
        {
            for( int col = 0 ; col <a[i] ; col++)
            {
                printf("*");
            }
            printf("\n");
        }
        if (i < T - 1) 
            printf("\n");
    }
    return 0;
}
