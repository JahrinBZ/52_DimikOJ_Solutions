#include <stdio.h>
int a;
int even_odd(int num){
    if (num == 0 )
        return 0;
    else if (num % 2 == 0)
        return 0;
    else
        return 1;
}
void input_taking(){
    scanf("%d",&a);
}
int main(){
    int T;
    scanf ("%d",&T);
    for (int i = 0; i<T ; i++){
    input_taking();
    int result = even_odd(a);
    if (result == 0)
        printf("even\n");
    else 
        printf("odd\n");
    }
    return 0;
}
